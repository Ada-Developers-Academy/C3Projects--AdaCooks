<%= link_to "Add a new Ingredient", new_user_ingredient_path(session[:user_id])%>
<% @ingredients.each do |ingredient| %>
  <%= ingredient.name %>
  <%= image_tag(ingredient.image) %>
  <%= link_to "Edit Ingredient", edit_user_ingredient_path(session[:user_id], ingredient.id) %>
  <%= link_to "Delete Ingredient", user_ingredient_path(session[:user_id], ingredient.id), method: :delete, data: {confirm: "Are you sure you want to delete #{ingredient.name} forever?"} %>
<% end %>