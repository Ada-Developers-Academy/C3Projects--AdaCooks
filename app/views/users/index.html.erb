<h1>Meet Our Chefs!</h1>

  <div class="container-fluid">
    <% @users.each do |user| %>
      <div class="col-xs-6 col-md-4 search_result_tiles">
        <%= link_to(image_tag(user.image_url(:thumb)), user_path(user)) %>
        <br>
        <h4><%= link_to user.username, user_path(user) %></h4>
      </div>
    <% end %>
  </div>
