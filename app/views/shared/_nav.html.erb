<nav >
  <%= button_to "View All Recipes", recipes_path  %>
  <%= button_to "View All Ingredients", ingredients_path %>
  <% if session[:user_id].nil?  %>
    <li><%= button_to "Login", login_path, method: :get, class: "btn btn-info navbar-btn" %></li>
    <li><%= link_to "Sign Up", new_user_path, class: "navbar-link" %></li>
  <% else %>
    <li><%= button_to "Logout", logout_path, method: :delete, class: "btn btn-info navbar-btn" %></li>
    <li><%= link_to "Store Dashboard", user_path(session[:user_id]), class: "navbar-link" %></li>
  <% end %>

</nav>
