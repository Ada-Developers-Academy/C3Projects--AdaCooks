<h1>Login</h1>
  <% if flash.now[:login_error] %>
<div class="alert alert-danger">
    <%= flash.now[:login_error] %>
</div>
  <% end %>
<%= form_for :session, url: login_path do |f| %>
<legend>User Info</legend>
  <fieldset>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "LOGIN" %>
</fieldset>

<% end %>

